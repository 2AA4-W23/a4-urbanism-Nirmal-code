mvn install
cd generator
java -jar generator.jar sample.Mesh -i -np 300 -relation 25 -wth 500 -hth 500
ls -lh sample.mesh
cd ..
cd island
java -jar island.jar -i ../generator/sample.mesh -o sandbox.mesh --mode regular --shape irregular --altitude volcanic --biomes dessert
cd ..
cd visualizer
java -jar visualizer.jar ../island/sandbox.mesh sample.svg
ls -lh sample.svg