mvn install
cd generator
java -jar generator.jar sample.Mesh -i -np 1500 -relation 25 -wth 500 -hth 500
ls -lh sample.mesh
cd ..
cd island
java -jar island.jar -i ../generator/sample.mesh -o sandbox.mesh --mode regular --shape circle --altitude hills --biomes tropicalforrest --lakes 3 --rivers 15 -soil wet --aquifers 5 -se -6141758803403002661
cd ..
cd visualizer
java -jar visualizer.jar ../island/sandbox.mesh sample.svg
ls -lh sample.svg